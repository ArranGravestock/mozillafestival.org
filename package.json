{
  "name": "mozillafestival.org",
  "version": "1.0.0",
  "description": "Mozfest site",
  "author": "Mozilla Foundation",
  "repository": "mozilla/mozillafestival.org",
  "license": "MPL-2.0",
  "scripts": {
    "start": "npm-run-all --parallel server watch:js watch:css copy-static",
    "build:production": "npm-run-all --parallel minify:js minify:css copy-static",
    "build:dev": "npm-run-all --parallel build:js build:css copy-static",
    "build:js": "webpack --progress --profile --colors",
    "build:css": "npm run watch:css -- --no-watch",
    "copy-static": "npm-run-all --parallel copy:tabzilla copy:mofo-bootstrap",
    "copy:tabzilla": "mkdirp public/vendor && cp -r node_modules/mozilla-tabzilla public/vendor/mozilla-tabzilla",
    "copy:mofo-bootstrap": "mkdirp public/vendor && cp -r node_modules/mofo-bootstrap public/vendor/mofo-bootstrap",
    "minify:js": "npm run build:js -- --optimize-minimize --optimize-dedupe",
    "minify:css": "npm run watch:css -- --no-watch",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "autoless --autoprefix '> 1%, last 2 versions' less public",
    "postinstall": "mkdirp last && mkdirp public/pages && node build.js",
    "server": "node app.js"
  },
  "dependencies": {
    "autoless": "0.1.7",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "body-parser": "1.12.4",
    "compression": "1.4.4",
    "cross-spawn": "0.2.9",
    "css-loader": "0.14.2",
    "express": "3.4.5",
    "express-rate-limit": "1.0.2",
    "extract-text-webpack-plugin": "0.8.0",
    "file-loader": "0.8.4",
    "google-spreadsheet": "^1.0.1",
    "habitat": "0.4.1",
    "hatchet": "0.3.2",
    "json-loader": "0.5.1",
    "jsx-loader": "0.13.2",
    "mapbox.js": "2.1.5",
    "mkdirp": "^0.5.1",
    "mofo-bootstrap": "^3.2.0",
    "mofo-ui": "^1.6.1",
    "moment": "^2.10.3",
    "mozilla-tabzilla": "^0.5.1",
    "node-libs-browser": "0.5.2",
    "npm-run-all": "1.2.4",
    "react": "15.4.2",
    "react-dom": "^15.4.2",
    "react-fa": "^4.1.2",
    "react-formation": "^0.3.1",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "request": "2.55.0",
    "slug": "^0.9.1",
    "style-loader": "0.12.3",
    "underscore": "^1.8.3",
    "url-loader": "0.5.6",
    "uuid": "^2.0.2",
    "webpack": "1.9.9",
    "whatwg-fetch": "0.9.0"
  },
  "engines": {
    "node": "^4.7.7"
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.1",
    "mofo-style": "^2.4.0"
  }
}
